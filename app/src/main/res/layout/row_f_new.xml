<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layoutDirection="rtl"
    android:orientation="vertical"
    android:layout_height="25dp">
    <View
        android:layout_width="0.5dp"
        android:layout_height="20dp"
        android:background="#E74949"
        android:gravity="center"
        android:layout_marginRight="23dp"
        />
    <View
        android:layout_width="0.5dp"
        android:layout_height="20dp"
        android:background="#E74949"
        android:gravity="center"
        android:layout_marginRight="85dp"
        />
    <View
        android:layout_width="0.5dp"
        android:layout_height="20dp"
        android:background="#E74949"
        android:gravity="center"
        android:layout_marginRight="135dp"
        />
    <View
        android:layout_width="0.5dp"
        android:layout_height="20dp"
        android:background="#E74949"
        android:gravity="center"
        android:layout_marginRight="205dp"
        />
    <View
        android:layout_width="0.5dp"
        android:layout_height="20dp"
        android:background="#E74949"
        android:gravity="center"
        android:layout_marginRight="245dp"
        />
    <View
        android:layout_width="0.5dp"
        android:layout_height="20dp"
        android:background="#E74949"
        android:gravity="center"
        android:layout_marginRight="290dp"
        />
    <View
        android:layout_width="0.5dp"
        android:layout_height="20dp"
        android:background="#E74949"
        android:gravity="center"
        android:layout_marginRight="335dp"
        />
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">


        <TextView
            android:layout_width="20dp"
            android:layout_height="wrap_content"
            android:id="@+id/shoot_tslsl"
            android:text="13"
            android:layout_marginRight="5dp"
            android:singleLine="true"
            android:textSize="12dp"
            android:textStyle="bold"
            android:textColor="#000000"
            />
        <TextView
            android:layout_width="50dp"
            android:singleLine="true"
            android:layout_height="wrap_content"
            android:id="@+id/shoot_code"
            android:textAlignment="textEnd"
            android:textSize="12dp"
            android:text="1023933"
            android:textStyle="bold"
            android:textColor="#000000"
            android:layout_marginRight="30dp"
            />
        <TextView
            android:layout_width="40dp"
            android:textSize="12dp"
            android:layout_height="wrap_content"
            android:id="@+id/shoot_deps"
            android:singleLine="true"
            android:textAlignment="textEnd"
            android:text="10293"
            android:textStyle="bold"
            android:textColor="#000000"
            android:layout_marginRight="90dp"
            />
        <TextView
            android:layout_width="65dp"
            android:layout_height="wrap_content"
            android:id="@+id/shoot_name"
            android:text="102939"
            android:singleLine="true"
            android:textStyle="bold"
            android:textColor="#000000"
            android:textSize="12dp"
            android:layout_marginRight="140dp"
            />
        <TextView
            android:layout_width="30dp"
            android:layout_height="wrap_content"
            android:id="@+id/shoot_number"
            android:singleLine="true"
            android:textSize="12dp"
            android:text="102"
            android:textStyle="bold"
            android:textColor="#000000"
            android:layout_marginRight="210dp"
            />

        <TextView
            android:layout_width="40dp"
            android:layout_height="wrap_content"
            android:id="@+id/shoot_unite"
            android:text="كارتونة"
            android:singleLine="true"
            android:textSize="12dp"
            android:textStyle="bold"
            android:textColor="#000000"
            android:layout_marginRight="250dp"
            />
        <TextView
            android:layout_width="40dp"
            android:layout_height="wrap_content"
            android:id="@+id/shoot_price"
            android:text="10293"
            android:singleLine="true"
            android:textStyle="bold"
            android:textColor="#000000"
            android:textSize="12dp"
            android:layout_marginRight="295dp"
            />
        <TextView
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:textSize="12dp"
            android:singleLine="true"
            android:id="@+id/shoot_toal"
            android:text="10293"
            android:textStyle="bold"
            android:textColor="#000000"
            android:layout_marginRight="350dp"
            />



    </RelativeLayout>



</RelativeLayout>