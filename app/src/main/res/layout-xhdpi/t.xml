<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layoutDirection="rtl"
    android:orientation="vertical"
    android:layout_height="15dp">
    <View
        android:layout_width="0.2dp"
        android:layout_height="20dp"
        android:background="#E74949"
        android:gravity="center"
        android:layout_marginRight="17dp"
        />
    <View
        android:layout_width="0.2dp"
        android:layout_height="20dp"
        android:background="#E74949"
        android:gravity="center"
        android:layout_marginRight="59dp"
        />
    <View
        android:layout_width="0.2dp"
        android:layout_height="20dp"
        android:background="#E74949"
        android:gravity="center"
        android:layout_marginRight="102dp"
        />
    <View
        android:layout_width="0.2dp"
        android:layout_height="20dp"
        android:background="#E74949"
        android:gravity="center"
        android:layout_marginRight="165dp"
        />
    <View
        android:layout_width="0.2dp"
        android:layout_height="20dp"
        android:background="#E74949"
        android:gravity="center"
        android:layout_marginRight="205dp"
        />
    <View
        android:layout_width="0.2dp"
        android:layout_height="20dp"
        android:background="#E74949"
        android:gravity="center"
        android:layout_marginRight="245dp"
        />
    <View
        android:layout_width="0.2dp"
        android:layout_height="20dp"
        android:background="#E74949"
        android:gravity="center"
        android:layout_marginRight="305dp"
        />
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">


        <TextView
            android:layout_width="10dp"
            android:layout_height="wrap_content"
            android:id="@+id/shoot_tslsl"
            android:text="13"
            android:layout_marginRight="5dp"
            android:singleLine="true"
            android:textSize="9dp"
            android:textStyle="bold"
            android:textColor="#000000"
            />
        <TextView
            android:layout_width="35dp"
            android:singleLine="true"
            android:layout_height="wrap_content"
            android:id="@+id/shoot_code"
            android:textAlignment="textEnd"
            android:textSize="9dp"
            android:text="1023933"
            android:textStyle="bold"
            android:textColor="#000000"
            android:layout_marginRight="20dp"
            />
        <TextView
            android:layout_width="35dp"
            android:textSize="9dp"
            android:layout_height="wrap_content"
            android:id="@+id/shoot_deps"
            android:singleLine="true"
            android:textAlignment="textEnd"
            android:text="10293"
            android:textStyle="bold"
            android:textColor="#000000"
            android:layout_marginRight="65dp"
            />
        <TextView
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:id="@+id/shoot_name"
            android:text="102939"
            android:singleLine="true"
            android:textStyle="bold"
            android:textColor="#000000"
            android:textSize="9dp"
            android:layout_marginRight="110dp"
            />
        <TextView
            android:layout_width="30dp"
            android:layout_height="wrap_content"
            android:id="@+id/shoot_number"
            android:singleLine="true"
            android:textSize="9dp"
            android:text="102"
            android:textStyle="bold"
            android:textColor="#000000"
            android:layout_marginRight="170dp"
            />

        <TextView
            android:layout_width="30dp"
            android:layout_height="wrap_content"
            android:id="@+id/shoot_unite"
            android:text="كارتونة"
            android:singleLine="true"
            android:textSize="9dp"
            android:textStyle="bold"
            android:textColor="#000000"
            android:layout_marginRight="210dp"
            />
        <TextView
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:id="@+id/shoot_price"
            android:text="10293"
            android:singleLine="true"
            android:textStyle="bold"
            android:textColor="#000000"
            android:textSize="9dp"
            android:layout_marginRight="250dp"
            />
        <TextView
            android:layout_width="55dp"
            android:layout_height="wrap_content"
            android:textSize="9dp"
            android:singleLine="true"
            android:id="@+id/shoot_toal"
            android:text="10293"
            android:textStyle="bold"
            android:textColor="#000000"
            android:layout_marginRight="310dp"
            />

    </RelativeLayout>



</RelativeLayout>