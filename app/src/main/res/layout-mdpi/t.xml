        android:layout_width="0.2dp"
        android:layout_height="20dp"
        android:background="#E74949"
        android:gravity="center"
        android:layout_marginRight="400dp"
        />
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">


        <TextView
            android:layout_width="30dp"
            android:layout_height="wrap_content"
            android:id="@+id/shoot_tslsl"
            android:text="13"
            android:layout_marginRight="5dp"
            android:singleLine="true"
            android:textSize="10dp"
            android:textStyle="bold"
            android:textColor="#000000"
            />
        <TextView
            android:layout_width="50dp"
            android:singleLine="true"
            android:layout_height="wrap_content"
            android:id="@+id/shoot_code"
            android:textAlignment="textEnd"
            android:textSize="10dp"
            android:text="1023933"
            android:textStyle="bold"
            android:textColor="#000000"
            android:layout_marginRight="30dp"
            />
        <TextView
            android:layout_width="40dp"
            android:textSize="10dp"
            android:layout_height="wrap_content"
            android:id="@+id/shoot_deps"
            android:singleLine="true"
            android:textAlignment="textEnd"
            android:text="10293"
            android:textStyle="bold"
            android:textColor="#000000"
            android:layout_marginRight="86dp"
            />
        <TextView
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:id="@+id/shoot_name"
            android:text="102939"
            android:singleLine="true"
            android:textStyle="bold"
            android:textColor="#000000"
            android:textSize="10dp"
            android:layout_marginRight="140dp"
            />
        <TextView
            android:layout_width="40dp"
            android:layout_height="wrap_content"
            android:id="@+id/shoot_number"
            android:singleLine="true"
            android:textSize="10dp"
            android:text="102"
            android:textStyle="bold"
            android:textColor="#000000"
            android:layout_marginRight="230dp"
            />

        <TextView
            android:layout_width="40dp"
            android:layout_height="wrap_content"
            android:id="@+id/shoot_unite"
            android:text="كارتونة"
            android:singleLine="true"
            android:textSize="10dp"
            android:textStyle="bold"
            android:textColor="#000000"
            android:layout_marginRight="278dp"
            />
        <TextView
            android:layout_width="70dp"
            android:layout_height="wrap_content"
            android:id="@+id/shoot_price"
            android:text="10293"
            android:singleLine="true"
            android:textStyle="bold"
            android:textColor="#000000"
            android:textSize="10dp"
            android:layout_marginRight="325dp"
            />
        <TextView
            android:layout_width="70dp"
            android:layout_height="wrap_content"
            android:textSize="10dp"
            android:singleLine="true"
            android:id="@+id/shoot_toal"
            android:text="10293"
            android:textStyle="bold"
            android:textColor="#000000"
            android:layout_marginRight="405dp"
            />

    </RelativeLayout>



</RelativeLayout>